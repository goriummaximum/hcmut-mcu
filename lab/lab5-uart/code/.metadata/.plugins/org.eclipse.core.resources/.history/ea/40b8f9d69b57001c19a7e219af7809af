#include "main.h"
#include "uart_communication_fsm.h"

enum CommandState {
	BEGIN,
	OK,
	RST
};

static enum CommandState commandState = BEGIN;

void uart_communication_fsm(void) {
	switch(commandState) {
	case BEGIN:
		break;

	case OK:
		break;

	case RST:
		break;
	}
}
