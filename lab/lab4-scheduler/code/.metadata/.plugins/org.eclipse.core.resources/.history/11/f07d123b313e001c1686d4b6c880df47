void task0(void) {
	HAL_GPIO_TogglePin(GPIOA, GPIO_Pin_0);
}

void task1(void) {
	HAL_GPIO_TogglePin(GPIOA, GPIO_Pin_1);
}

void task2(void) {
	HAL_GPIO_TogglePin(GPIOA, GPIO_Pin_2);
}

void task3(void) {
	HAL_GPIO_TogglePin(GPIOA, GPIO_Pin_3);
}

void task4(void) {
	HAL_GPIO_TogglePin(GPIOA, GPIO_Pin_4);
}
